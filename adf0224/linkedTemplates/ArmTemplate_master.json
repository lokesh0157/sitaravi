{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf0224"
		},
		"AzureBlobStoragein_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragein'"
		},
		"AzureBlobStoragenewin_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragenewin'"
		},
		"AzureBlobStorageout_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageout'"
		},
		"AzureBlobStorageoutputnew_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageoutputnew'"
		},
		"testdatasetin_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'testdatasetin'"
		},
		"testdatasetout_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'testdatasetout'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf0224_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStoragenewin_connectionString": {
						"value": "[parameters('AzureBlobStoragenewin_connectionString')]"
					},
					"AzureBlobStorageoutputnew_connectionString": {
						"value": "[parameters('AzureBlobStorageoutputnew_connectionString')]"
					},
					"testdatasetin_connectionString": {
						"value": "[parameters('testdatasetin_connectionString')]"
					},
					"testdatasetout_connectionString": {
						"value": "[parameters('testdatasetout_connectionString')]"
					},
					"AzureBlobStoragein_connectionString": {
						"value": "[parameters('AzureBlobStoragein_connectionString')]"
					},
					"AzureBlobStorageout_connectionString": {
						"value": "[parameters('AzureBlobStorageout_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}